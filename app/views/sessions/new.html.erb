<h1> New User Registration </h1>
<p><%=  link_to "Return to Movie Viewing Party Landing Page", root_path %></p>
<%= form_with url: sessions_path, method: :post, data: {turbo: false}, local: true do |f|%>
  <%= f.label  :email %>
  <%= f.text_field :email %>
  
  <%= f.label  :name %>
  <%= f.text_field :name %>

  <%= f.label  :password %>
  <%= f.text_field :password %>

  <%= f.label  :password_confirmation %>
  <%= f.text_field :password_confirmation %>
  
  <%= f.submit "Register" %>
<% end %>
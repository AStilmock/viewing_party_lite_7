<div id="title">
  <h1> Movie Viewing Party! </h1>
  <p>Existing Customers Login Here: <%=  link_to "Login", login_path %></p>
  <p><%=  button_to "Create New User", new_user_path, method: :get %></p>
  <p><%=  link_to "Return to Movie Viewing Party Landing Page" %></p>
</div>

<div id="users">
  <h2> Movie Viewing Users: </h2>
  <p><%  @users.each do |person| %></p>
    <p><%= link_to "#{person.name}", user_path(person.id) %>
  <p><%  end %></p>
</div>